{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "browser": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "arrow-parens": 2,
        "arrow-spacing": [2, { "before": true, "after": true }],
        "constructor-super": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-console": 0,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-path-concat": 2,
        "no-plusplus": 2,
        "no-this-before-super": 2,
        "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
        "no-var": 2,
        "object-shorthand": 2,
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "prefer-spread": 2,
        "prefer-template": 2,
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "strict": 0
    }
}
